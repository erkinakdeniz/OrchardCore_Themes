    {
      "name": "Templates",
      "Templates": {
        "Content__LandingPage": {
          "Content": "<header class=\"masthead\">\r\n \r\n            <div class=\"container h-100\">\r\n            {% for header in Model.ContentItem.Content.Header.ContentItems %}\r\n                <div class=\"row h-100 align-items-center justify-content-center text-center\">\r\n                    <div class=\"col-lg-10 align-self-end\">\r\n                        <h1 class=\"text-uppercase text-white font-weight-bold\">{{ header.Header.HeadTitle.Text }}</h1>\r\n                        <hr class=\"divider my-4\" />\r\n                    </div>\r\n                    <div class=\"col-lg-8 align-self-baseline\">\r\n                      \r\n                        <p class=\"text-white-75 font-weight-light mb-5\">{{ header.Header.Description.Text }}</p>\r\n                        <a class=\"btn btn-primary btn-xl js-scroll-trigger\" href=\"{{header.Header.Link.Url}}\">{{header.Header.Link.Text}}</a>\r\n                    </div>\r\n                </div>\r\n             {%endfor%}\r\n            </div>\r\n        </header>\r\n<!-- About-->\r\n        <section class=\"page-section bg-primary\" id=\"about\">\r\n            <div class=\"container\">\r\n                <div class=\"row justify-content-center\">\r\n                  {%for about in Model.ContentItem.Content.About.ContentItems%}\r\n                    <div class=\"col-lg-8 text-center\">\r\n                      \r\n                        <h2 class=\"text-white mt-0\">{{ about.About.HeadTitle.Text }}</h2>\r\n                        <hr class=\"divider light my-4\" />\r\n                        <p class=\"text-white-50 mb-4\">{{ about.About.Description.Text }}</p>\r\n                        <a class=\"btn btn-light btn-xl js-scroll-trigger\" href=\"{{ about.About.Link.Url }}\">{{ about.About.Link.Text }}</a>\r\n                    </div>\r\n                </div>\r\n              {%endfor%}\r\n            </div>\r\n        </section>\r\n        <!-- Services-->\r\n        <section class=\"page-section\" id=\"services\">\r\n            <div class=\"container\">\r\n             \r\n                <h2 class=\"text-center mt-0\">At Services</h2>\r\n                <hr class=\"divider my-4\" />\r\n                <div class=\"row\">\r\n                   {%for service in Model.ContentItem.Content.Services.ContentItems%}\r\n                    <div class=\"col-lg-3 col-md-6 text-center\">\r\n                        <div class=\"mt-5\">\r\n                            <i class=\"{{service.Service.IconClass.Text}}\"></i>\r\n                          <h3 class=\"h4 mb-2\">{{service.TitlePart.Title}}</h3>\r\n                          <p class=\"text-muted mb-0\">{{service.HtmlBodyPart.Html}}</p>\r\n                        </div>\r\n                    </div>\r\n                    {%endfor%}\r\n                </div>\r\n              \r\n            </div>\r\n        </section>\r\n\r\n        <!-- Portfolio-->\r\n        <div id=\"portfolio\">\r\n            <div class=\"container-fluid p-0\">\r\n                <div class=\"row no-gutters\">\r\n                  {%for portfolio in Model.ContentItem.Content.Portfolio.ContentItems%}\r\n                    <div class=\"col-lg-4 col-sm-6\">\r\n                        <a class=\"portfolio-box\" href=\"/media/{{portfolio.Project.Image.Paths}}?width=240&amp;height=240\">\r\n                            <img class=\"img-fluid\" src=\"/media/{{portfolio.Project.Image.Paths}}\" alt=\"{{portfolio.Project.Image.MediaTexts}}\" />\r\n                            <div class=\"portfolio-box-caption\">\r\n                                <div class=\"project-category text-white-50\">{{portfolio.Project.Category.Text}}</div>\r\n                                <div class=\"project-name\">{{portfolio.TitlePart.Title}}</div>\r\n                            </div>\r\n                        </a>\r\n                    </div>\r\n                    {%endfor%}\r\n                    \r\n                </div>\r\n            </div>\r\n        </div>\r\n        <!-- Call to action-->\r\n        <section class=\"page-section bg-dark text-white\" id=\"download\">\r\n            <div class=\"container text-center\">\r\n              {%for down in Model.ContentItem.Content.Download.ContentItems%}\r\n                <h2 class=\"mb-4\">{{down.Download.Content.Text}}</h2>\r\n                <a class=\"btn btn-light btn-xl\" href=\"{{down.Download.Link.Url}}\">{{down.Download.Link.Text}}</a>\r\n              {%endfor%}\r\n            </div>\r\n        </section>\r\n        <!-- Contact-->\r\n        <section class=\"page-section\" id=\"contact\">\r\n            <div class=\"container\">\r\n              {%for down in Model.ContentItem.Content.Contact.ContentItems%}\r\n                <div class=\"row justify-content-center\">\r\n                    <div class=\"col-lg-8 text-center\">\r\n                        <h2 class=\"mt-0\">{{down.Contact.HeadTitle.Text}}</h2>\r\n                        <hr class=\"divider my-4\" />\r\n                        <p class=\"text-muted mb-5\">{{down.Contact.Description.Text}}</p>\r\n                    </div>\r\n                </div>\r\n                <div class=\"row\">\r\n                  \r\n                    <div class=\"col-lg-4 ml-auto text-center mb-5 mb-lg-0\">\r\n                        <i class=\"{{down.Contact.PhoneIcon.Text}}\"></i>\r\n                        <div>{{down.Contact.PhoneNumber.Text}}</div>\r\n                    </div>\r\n                    <div class=\"col-lg-4 mr-auto text-center\">\r\n                        <i class=\"{{down.Contact.MailIcon.Text}}\"></i>\r\n                        <!-- Make sure to change the email address in BOTH the anchor text and the link target below!-->\r\n                        <a class=\"d-block\" href=\"mailto:{{down.Contact.MailAddress.Text}}\">{{down.Contact.MailAddress.Text}}</a>\r\n                    </div>\r\n                </div>\r\n              {%endfor%}\r\n            </div>\r\n        </section>",
          "Description": "A template for the Landing Page content type"
        }
      }
